{
  "name": "@jlongyam/cli",
  "version": "1.0.3",
  "description": "CLI scripts collection",
  "keywords": [
    "cli"
  ],
  "homepage": "https://github.com/jlongyam/cli#readme",
  "bugs": {
    "url": "https://github.com/jlongyam/cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlongyam/cli.git"
  },
  "license": "MIT",
  "author": "jlongyam",
  "main": "./dist/index.js",
  "type": "module",
  "exports": {
    "import": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "args-parser": "^1.3.0",
    "clean-jsdoc-theme": "^4.3.0",
    "enquirer": "^2.4.1",
    "jsdoc": "^4.0.4",
    "rollup": "^4.45.3"
  },
  "scripts": {
    "build": "clear && rollup -c",
    "test:args": "clear && node ./test/args.js",
    "test:args:length": "clear && node ./test/args.js -h --version name=test",
    "test:dist:args": "clear && node ./test/dist/args.js",
    "test:dist:args:length": "clear && node ./test/dist/args.js -h --version name=test",
    "test:input:ask": "clear && node ./test/input.ask.js",
    "test:dist:input:ask": "clear && node ./test/dist/input.ask.js",
    "test:input:asks": "clear && node ./test/input.asks.js",
    "test:dist:input:asks": "clear && node ./test/dist/input.asks.js",
    "test:index": "clear && node ./test/index.js",
    "test:dist:index": "clear && node ./test/dist/index.js",
    "doc": "jsdoc ./src/ -d ./docs/ -t ./node_modules/clean-jsdoc-theme -R ./README.md",
    "update": "npm publish",
    "next": "npm version patch"
  }
}
