{
  "source": {
    "include": [
      "./src",
      "./README.md"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "./plugins/markdown"
  ],
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "./docs",
    "recurse": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "dark",
      "homepageTitle": "cli - home",
      "title": "HOME",
      "menu": [
        {
          "title": "Github",
          "id": "github",
          "link": "https://github.com/jlongyam/cli/"
        },
        {
          "title": "npm",
          "id": "npm",
          "link": "https://www.npmjs.com/package/@jlongyam/cli"
        }
      ]
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}